<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="finalproject.ski2rent">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Ski2Rent">
        <activity android:name=".activities.Activity_Login"></activity>
        <activity android:name=".activities.Activity_ShoppingCart" />
        <activity android:name=".activities.Activity_OrdersStatus" />
        <activity android:name=".activities.Activity_RentSnowboards" />
        <activity android:name=".activities.Activity_RentSkis" />
        <activity android:name=".activities.Activity_Prices" />
        <activity android:name=".activities.Activity_MainMenu">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>